¡Claro! Aquí te muestro una posible estructura para el objeto Comprobante en Kotlin:
data class Comprobante(
    val version: String,
    val serie: String,
    val folio: String,
    val fecha: String,
    val formaPago: String,
    val subTotal: Double,
    val descuento: Double,
    val moneda: String,
    val tipoCambio: Int,
    val total: Double,
    val tipoDeComprobante: String,
    val metodoPago: String,
    val lugarExpedicion: String,
    val emisor: Emisor,
    val receptor: Receptor,
    val conceptos: List<Concepto>,
    val timbreFiscalDigital: TimbreFiscalDigital
)

data class Emisor(
    val rfc: String,
    val nombre: String,
    val regimenFiscal: String
)

data class Receptor(
    val rfc: String,
    val nombre: String,
    val usoCFDI: String
)

data class Concepto(
    val claveProdServ: String,
    val cantidad: Int,
    val claveUnidad: String,
    val descripcion: String,
    val valorUnitario: Double,
    val importe: Double,
    val descuento: Double
)

data class TimbreFiscalDigital(
    val uuid: String,
    val fechaTimbrado: String,
    val rfcProvCertif: String,
    val selloCFD: String,
    val noCertificadoSAT: String,
    val selloSAT: String
)

val comprobante = Comprobante(
    version = "3.3",
    serie = "AVM",
    folio = "0000359",
    fecha = "2019-09-12T08:09:24",
    formaPago = "99",
    subTotal = 12120.17,
    descuento = 2656.30,
    moneda = "MXN",
    tipoCambio = 1,
    total = 9463.87,
    tipoDeComprobante = "N",
    metodoPago = "PUE",
    lugarExpedicion = "04200",
    emisor = Emisor(
        rfc = "GAV9708211W4",
        nombre = "GNP ADMINISTRACION DE VENTA MASIVA, S.A. DE C.V.",
        regimenFiscal = "601"
    ),
    receptor = Receptor(
        rfc = "AOHH9803252Q5",
        nombre = "HAROLD ALONSO HERNANDEZ",
        usoCFDI = "P01"
    ),
    conceptos = listOf(
        Concepto(
            claveProdServ = "84111505",
            cantidad = 1,
            claveUnidad = "ACT",
            descripcion = "Pago de nómina",
            valorUnitario = 12120.17,
            importe = 12120.17,
            descuento = 2656.30
        )
    ),
    timbreFiscalDigital = TimbreFiscalDigital(
        uuid = "25D60C03-49F1-4D4A-9AE7-426B6D6CA1B1",
